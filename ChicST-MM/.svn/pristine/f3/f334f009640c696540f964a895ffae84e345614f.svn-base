        function checkboxText(inputM, visitMan) {
            //多选框值
            var checkboxS = visitMan.find("input.active");
            var htmlStr = '';
            $(checkboxS).each(function (i, m) {
                if (m.value != null) {
                    htmlStr += m.value + '&';
                }

            })
            inputM.val(htmlStr);
        }
