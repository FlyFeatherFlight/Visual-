        /* 二级菜单 */
        $('.type-list').each(function(i, li) {
            li.onclick = function () {
                $(this).next('.nav-second').slideToggle();
                $(this).children('.glyphicon-menu-left').toggleClass('running');
                $(this).addClass('active').siblings().removeClass('active');
            }
        })

        //时间选择器
