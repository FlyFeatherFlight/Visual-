@{
    ViewBag.Title = "Home Page";
}

<div class="index-bg">
    <div class="mask"></div>
    <div class="logo-warpper">
        <div class="logo-img">
            <img src="~/images/new.jpg" style="width: 120px;" />

        </div>
        <div class="logo-aim ">选择&nbsp;&nbsp;探索&nbsp;&nbsp;改变</div>
    </div>
    <section id="dg-container" class="dg-container">
        <div class="dg-wrapper">
            <a href="javascript:;"><img src="images/1.jpg" alt="" onerror="nofind()"></a>
            <a href="javascript:;"><img src="images/2.jpg" alt="" onerror="nofind()"></a>
            <a href="javascript:;"><img src="images/3.jpg" alt="" onerror="nofind()"></a>
            <a href="javascript:;"><img src="images/4.jpg" alt="" onerror="nofind()"></a>
            <a href="javascript:;"><img src="images/5.jpg" alt="" onerror="nofind()"></a>
            <a href="javascript:;"><img src="images/6.jpg" alt="" onerror="nofind()"></a>
            <a href="javascript:;"><img src="images/7.jpg" alt="" onerror="nofind()"></a>
            <a href="javascript:;"><img src="images/8.jpg" alt="" onerror="nofind()"></a>
            <a href="javascript:;"><img src="images/9.jpg" alt="" onerror="nofind()"></a>
            <a href="javascript:;"><img src="images/10.jpg" alt="" onerror="nofind()"></a>
            <a href="javascript:;"><img src="images/11.jpg" alt="" onerror="nofind()"></a>
            <a href="javascript:;"><img src="images/12.jpg" alt="" onerror="nofind()"></a>
        </div>
    </section>
</div>
<script type="text/javascript">
   //图片未显示则显示默认图片
    function nofind(){
        var img=event.srcElement;
            img.src ="images/1.jpg"; //替换的图片
        img.onerror=null; //控制不要一直触发错误
    }


	$(function() {
		$('#dg-container').gallery({
			autoplay:true
		});
    });

</script>