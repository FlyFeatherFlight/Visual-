@model IEnumerable<ChicST_MM.WEB.Models.Development_MarketRentalViewModel>

@{ 
    Layout = null;
}
<style>
    .table tbody tr td:nth-child(2n-1) {
        text-align: center;
        width: auto;
    }
</style>
<table class="table table-bordered tabMoney" style="font-size:12px;">
    <tr>

        <td>
            @Html.DisplayNameFor(model => model.租金年)
        </td>
        <td>
            @Html.DisplayNameFor(model => model.租金月)
        </td>
        <td>
           租金金额（万元）
        </td>
        <td>操作</td>
    </tr>

@if (Model.Count() > 0)
{
    foreach (var item in Model)
    {
    <tr>
        <td class="shopId hidden">
            @Html.DisplayFor(modelItem => item.商场ID)
        </td>
        <td class="Id hidden">
            @Html.DisplayFor(modelItem => item.ID)
        </td>
        <td class="year">
            @Html.DisplayFor(modelItem => item.租金年)
        </td>
        <td class="month">
            @Html.DisplayFor(modelItem => item.租金月)
        </td>
        <td class="money">
            @Html.DisplayFor(modelItem => item.租金金额)
        </td>
        <td>
            <span class="Aspan" onclick="Edit(this)">修改</span>
            <span class="Aspan" onclick="Del(this)">删除</span>
        </td>
    </tr>
    }
}
else {
<tr>
    <td colspan="4">暂无数据!</td>
</tr>
}</table>
