*{box-sizing: border-box;}
body,html{width: 100%;height: 100%;position: relative;padding:0;margin:0;
}
.fl{
float: left;
}
.fr{float: right;}
.clearFix:after{
    content:".";        
    display:block;        
    height:0;        
    clear:both;        
    visibility:hidden;    
}
.inline-block{ display: inline-block;
}
ul{list-style-type: none;margin: 0 ;padding: 0 ;}
h3 {
    margin-left:2.5%;
}
