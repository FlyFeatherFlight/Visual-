*{box-sizing: border-box;}
body,html{width: 100%;height: 100%;position: relative;padding:0;margin:0;
}
.fl{
float: left;
}
.fr{float: right;}
.clearFix:after{
    content:".";        
    display:block;        
    height:0;        
    clear:both;        
    visibility:hidden;    
}
.inline-block{ display: inline-block;
}
ul{list-style-type: none;margin: 0 ;padding: 0 ;}
h3 {
    margin-left:2.5%;
}
/*普通表格*/
.table-bordered > tbody > tr > th {
    text-align: center;
}
/*需要合并的表格*/
#tab tbody tr td:nth-child(2n-1) {
    text-align: center;
    width: auto !important;
}
@font-face {
    font-family: 'iconfont'; /* project id 1028803 */
    src: url('//at.alicdn.com/t/font_1028803_ng7fb0omiw8.eot');
    src: url('//at.alicdn.com/t/font_1028803_ng7fb0omiw8.eot?#iefix') format('embedded-opentype'), url('//at.alicdn.com/t/font_1028803_ng7fb0omiw8.woff2') format('woff2'), url('//at.alicdn.com/t/font_1028803_ng7fb0omiw8.woff') format('woff'), url('//at.alicdn.com/t/font_1028803_ng7fb0omiw8.ttf') format('truetype'), url('//at.alicdn.com/t/font_1028803_ng7fb0omiw8.svg#iconfont') format('svg');
}
.iconfont {
    font-family: 'iconfont';
    font-style:normal;
}
.green{
    color:green;
}
.red {
    color:red;
}
.show {
display:block;
}
/*查询框样式*/
.search-input {
    margin-right: 5px;
    padding: 5px 12px;
    font-size: 14px;
    line-height: 1.42857143;
    color: #555;
    background-color: #fff;
    background-image: none;
    border: 1px solid #ccc;
    border-radius: 4px;
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
    -webkit-transition: border-color ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;
    -o-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
    transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
}
.ellipsis {
    width: 100%;
    word-break: keep-all;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.bold {
    font-weight:600;
}
.text-center {
    text-align:center;
}
/*返回箭头样式*/
.back{
    vertical-align: middle;
    display: inline-block;
    height: 100%;
    line-height: 43px;
    width: 28px;
    text-align: center;
    cursor:pointer;
}
/*竖着字体样式*/
.vertical-font {
   display:inline-block;
   width:20px;
} 

/*附件上传*/
.add_div {
    width: auto;
    height: auto;
   
}

.file-list {
    height: auto;
    display: none;
    list-style-type: none;
}

.file-list img {
    max-width: 70px;
    vertical-align: middle;
}

.file-list .file-item {
    margin-bottom: 10px;
}


.file-list .file-item .file-del {
    display: inline-block;
    margin-left: 20px;
    margin-top: 5px;
    cursor: pointer;
}
/*文字垂直居中*/

.table-bordered > tbody > tr > th {
    vertical-align: middle;
}