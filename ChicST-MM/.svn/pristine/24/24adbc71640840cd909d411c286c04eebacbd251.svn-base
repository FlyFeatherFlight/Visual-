*{box-sizing: border-box;}
body,html{width: 100%;height: 100%;position: relative;padding:0;margin:0;
}
.fl{
float: left;
}
.fr{float: right;}
.clearFix:after{
    content:".";        
    display:block;        
    height:0;        
    clear:both;        
    visibility:hidden;    
}
.inline-block{ display: inline-block;
}
ul{list-style-type: none;margin: 0 ;padding: 0 ;}
h3 {
    margin-left:2.5%;
}
.table-bordered > tbody > tr > th {
    text-align: center;
}
@font-face {
    font-family: 'iconfont'; /* project id 1028803 */
    src: url('//at.alicdn.com/t/font_1028803_ng7fb0omiw8.eot');
    src: url('//at.alicdn.com/t/font_1028803_ng7fb0omiw8.eot?#iefix') format('embedded-opentype'), url('//at.alicdn.com/t/font_1028803_ng7fb0omiw8.woff2') format('woff2'), url('//at.alicdn.com/t/font_1028803_ng7fb0omiw8.woff') format('woff'), url('//at.alicdn.com/t/font_1028803_ng7fb0omiw8.ttf') format('truetype'), url('//at.alicdn.com/t/font_1028803_ng7fb0omiw8.svg#iconfont') format('svg');
}
.iconfont {
    font-family: 'iconfont';
    font-style:normal;
}
.green{
    color:green;
}
.width-7 {
    width:7%;
}
/*²éÑ¯¿òÑùÊ½*/
.search-input {
    margin-right: 5px;
    padding: 5px 12px;
    font-size: 14px;
    line-height: 1.42857143;
    color: #555;
    background-color: #fff;
    background-image: none;
    border: 1px solid #ccc;
    border-radius: 4px;
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
    -webkit-transition: border-color ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;
    -o-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
    transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
}
.ellipsis {
    width: 100%;
    word-break: keep-all;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.bold {
    font-weight:600;
}
.text-center {
    text-align:center;
}